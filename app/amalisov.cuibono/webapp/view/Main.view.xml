<mvc:View
    controllerName="amalisov.cuibono.controller.Main"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns:m="sap.m"
    xmlns:core="sap.ui.core"
>
    <m:Page id="page">
        <m:customHeader>
            <core:Fragment
                id="custombar"
                fragmentName="amalisov.cuibono.view.CustomBar"
                type="XML"
            />
        </m:customHeader>
        <m:IconTabBar
            id="idm:IconTabBarNoIcons"
            expanded="{device>/isNoPhone}"
            class="sapUiResponsiveContentPadding"
        >
            <m:items>
                <m:IconTabFilter
                    id="bonusTrancheTab"
                    text="Bonus Tranches"
                    key="bonusTranche"
                >
                    <mvc:XMLView
                        id="BonusTrancheContent"
                        viewName="amalisov.cuibono.view.BonusTranches"
                    />
                </m:IconTabFilter>
                <m:IconTabFilter
                    id="participantsTab"
                    text="Partcipants"
                    key="participants"
                >
                    <mvc:XMLView
                        id="ParticipantsContent"
                        viewName="amalisov.cuibono.view.Participants"
                    />
                </m:IconTabFilter>
            </m:items>
        </m:IconTabBar>
    </m:Page>
</mvc:View>
