<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:f="sap.f"
>
    <f:ShellBar
        id="fragmentshell"
        class="customShellBar"
        homeIcon="./images/infineonLogo.png"
        title="{i18n>appTitle}"
        homeIconPressed="onLogoClick"
    >
        <f:profile>
            <Avatar
                id="navbarImage"
                initials="RH"
                press="onLogoClick"
            />
        </f:profile>
    </f:ShellBar>
</core:FragmentDefinition>
