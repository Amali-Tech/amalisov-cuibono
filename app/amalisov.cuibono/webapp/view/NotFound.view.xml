<mvc:View
    controllerName="sap.ui.demo.basicTemplate.controller.NotFound"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
>
    <Page
        showHeader="false"
        class="sapUiContentPadding"
    >
        <content>
            <m:IllustratedMessage
                title="Page Not Found"
                description="The page you are looking for doesn't exist or an error occurred."
                illustrationType="sapIllus-PageNotFound"
                illustrationSize="Auto"
            >
                <m:items>
                    <Button
                        text="Go Back"
                        press="onNavBack"
                    />
                </m:items>
            </m:IllustratedMessage>
        </content>
    </Page>
</mvc:View>
