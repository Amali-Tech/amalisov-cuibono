<mvc:View
    controllerName="sap.ui.demo.basicTemplate.controller.NotFound"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
>
    <Page
        showHeader="false"
        class="sapUiContentPadding"
    >
        <content>
            <m:IllustratedMessage
                title="{i18n>Pagenotfound}"
                description="{i18n>notFoundDescription}"
                illustrationType="sapIllus-PageNotFound"
                illustrationSize="Auto"
            >
                <m:items>
                    <Button
                        text="{i18n>goback}"
                        press="onNavBack"
                    />
                </m:items>
            </m:IllustratedMessage>
        </content>
    </Page>
</mvc:View>
