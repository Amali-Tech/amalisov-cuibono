{
  "_version": "1.65.0",
  "sap.app": {
    "id": "amalisov.cuibono",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.15.0",
      "toolsId": "9b612c57-e7e4-4342-bb68-847e7a242f6d"
    },
    "dataSources": {
      "mainService": {
        "uri": "/api/v1/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "handleValidation": true,
    "dependencies": {
      "minUI5Version": "1.128.1",
      "libs": {
        "sap.m": {},
        "sap.f": {},
        "sap.ui.core": {},
        "sap.ui.layout": {},
        "sap.ui.comp": {},
        "sap.ui.fl": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "amalisov.cuibono.i18n.i18n"
        }
      },
      "trancheModel": {
        "type": "sap.ui.model.odata.v4.ODataModel",
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true,
          "groupId": "$direct",
          "metadataUrlParams": {
            "sap-documentation": "heading"
          }
        }
      },
      "tranche": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/data.json"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "amalisov.cuibono.view",
        "controlId": "app",
        "transition": "slide",
        "async": true,
        "controlAggregation": "pages",
        "bypassed": {
          "target": "notFound"
        }
      },
      "routes": [
        {
          "name": "RouteMain",
          "pattern": "tranches:?query:",
          "target": "TargetMain"
        },
        {
          "name": "AddEditTranche",
          "pattern": "addEdit:?query:",
          "target": "AddEditTranche"
        }
      ],
      "targets": {
        "TargetMain": {
          "viewId": "TargetMain",
          "transition": "slide",
          "viewName": "Main",
          "viewLevel": 0
        },
        "AddEditTranche": {
          "viewId": "AddEditTranche",
          "transition": "slide",
          "viewName": "AddEditTranche"
        },
        "BonusTranches": {
          "viewId": "bonusTranches",
          "parent": "TargetMain",
          "viewName": "BonusTranches",
          "controlId": "bonusTranchesTab",
          "controlAggregation": "content"
        },
        "Participants": {
          "viewId": "participants",
          "parent": "TargetMain",
          "viewName": "Participants",
          "controlId": "participantsTab",
          "controlAggregation": "content"
        },
        "notFound": {
          "viewId": "notFound",
          "viewName": "NotFound",
          "transition": "show"
        }
      }
    },
    "rootView": {
      "viewName": "amalisov.cuibono.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
